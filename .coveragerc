[run]
omit =
    tests/*
    */tests/*
    setup.py
    conftest.py
    */__pycache__/*
    *.pyc

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    @abstractmethod
